<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="modulepkg.tib.Miraz_2222649.policyAnalyst.policyAnalystController">
    <children>
        <Text fill="#f50606" layoutX="150.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Policy Advocacy Officer">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Text>

        <Button fx:id="logoutButton" layoutX="26.0" layoutY="14.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Logout" onAction="#handleLogout"/>

        <Button fx:id="reviewResearchReportsButton" layoutX="83.0" layoutY="81.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="172.0" text="Review Research Reports" onAction="#handleReviewResearchReports"/>

        <Button fx:id="publicFriendlyPolicyButton" layoutX="83.0" layoutY="146.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="172.0" text="Public-Friendly Policy" onAction="#handlePublicFriendlyPolicy"/>

        <Button fx:id="trackStatusButton" layoutX="83.0" layoutY="214.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="172.0" text="Track Status of Policy" onAction="#handleTrackStatus"/>

        <Button fx:id="publishFindingsButton" layoutX="83.0" layoutY="282.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="172.0" text="Publish Findings" onAction="#handlePublishFindings"/>

        <Button fx:id="suggestResearchTopicButton" layoutX="325.0" layoutY="81.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="172.0" text="Suggest Research Topic" onAction="#handleSuggestResearchTopic"/>

        <Button fx:id="planManageAdvocacyButton" layoutX="325.0" layoutY="146.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="172.0" text="Plan and Manage Advocacy" onAction="#handlePlanAndManageAdvocacy"/>

        <Button fx:id="communicateWithButton" layoutX="325.0" layoutY="214.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="172.0" text="Communicate with" onAction="#handleCommunicateWith"/>

        <Button fx:id="evaluateImpactButton" layoutX="325.0" layoutY="282.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="172.0" text="Evaluate Impact" onAction="#handleEvaluateImpact"/>
    </children>
</AnchorPane>
