<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="modulepkg.tib.Miraz_2222649.researchOfficer.researchOfficerController">
    <children>
        <Button fx:id="logoutButton" layoutX="14.0" layoutY="23.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="69.0" text="Logout" onAction="#handleLogout"/>

        <Button fx:id="newSurveyButton" layoutX="74.0" layoutY="99.0" mnemonicParsing="false"
                prefHeight="48.0" prefWidth="196.0" text="New Research Survey" onAction="#handleNewSurvey"/>

        <Button fx:id="analyzeDataButton" layoutX="74.0" layoutY="165.0" mnemonicParsing="false"
                prefHeight="48.0" prefWidth="196.0" text="Analyze Collected Data" onAction="#handleAnalyzeData"/>

        <Button fx:id="categorizeReportsButton" layoutX="74.0" layoutY="240.0" mnemonicParsing="false"
                prefHeight="48.0" prefWidth="196.0" text="Categorize Reports Sector" onAction="#handleCategorizeReports"/>

        <Button fx:id="submitReportsButton" layoutX="74.0" layoutY="313.0" mnemonicParsing="false"
                prefHeight="48.0" prefWidth="196.0" text="Submit Reports to Policy Team" onAction="#handleSubmitReports"/>

        <Button fx:id="collectInputDataButton" layoutX="331.0" layoutY="94.0" mnemonicParsing="false"
                prefHeight="48.0" prefWidth="196.0" text="Collect and Input Data" onAction="#handleCollectInputData"/>

        <Button fx:id="generateReportsButton" layoutX="331.0" layoutY="160.0" mnemonicParsing="false"
                prefHeight="48.0" prefWidth="196.0" text="Generate Research Reports" onAction="#handleGenerateReports"/>

        <Button fx:id="collaborateFieldStaffButton" layoutX="331.0" layoutY="235.0" mnemonicParsing="false"
                prefHeight="48.0" prefWidth="196.0" text="Collaborate with Field Staff" onAction="#handleCollaborateFieldStaff"/>

        <Button fx:id="storeManageRecordButton" layoutX="331.0" layoutY="308.0" mnemonicParsing="false"
                prefHeight="48.0" prefWidth="196.0" text="Store  Manage Record" onAction="#handleStoreManageRecord"/>

        <Text layoutX="124.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Research Officer Dashboard ">
            <font>
                <Font size="30.0" />
            </font>
        </Text>
    </children>
</AnchorPane>
